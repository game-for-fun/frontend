<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ball.dao.LotteryResultDao">
	<resultMap id="BaseResultMap" type="com.ball.bean.LotteryResult">
		<id column="id" property="id" jdbcType="INTEGER" />
		<result column="user_name" property="userName" jdbcType="VARCHAR" />
		<result column="character_name" property="characterName" jdbcType="VARCHAR" />
		<result column="create_time" property="createTime" jdbcType="DATE" />
	</resultMap>

	<insert id="addLotteryResult" parameterType="com.ball.bean.LotteryResult">
		INSERT INTO lottery_result ( user_name,character_name,create_time ) 
		VALUES ( #{userName},#{characterName},#{createTime} )
	</insert>
	
</mapper>